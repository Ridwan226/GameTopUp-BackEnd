<table id="example1" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>nama</th>
      <th>Qty</th>
      <th>Price</th>
      <th>Acction</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < nominal.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= nominal[i].coinName %></td>
      <td><%= nominal[i].coinQuantity %></td>
      <td><%= nominal[i].price %></td>
      <td>
        <form action="/nominal/del/<%= nominal[i]._id %>?_method=DELETE" method="post" >
          <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning"> <i class="fa fa-pencil-alt"></i></a>
          <button class="btn btn-danger"><i class="fa fa-trash"></i></button>
        </form>
      </td>
    </tr>
    <% } %>
    </tfoot>
</table>