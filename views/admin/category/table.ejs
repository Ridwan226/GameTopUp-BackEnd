<table id="example1" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Acction</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < category.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= category[i].name %></td>
      <td>
        <form action="/category/del/<%= category[i]._id %>?_method=DELETE" method="post" >
          <a href="/category/edit/<%= category[i]._id %>" class="btn btn-warning"> <i class="fa fa-pencil-alt"></i></a>
          <button class="btn btn-danger"><i class="fa fa-trash"></i></button>
        </form>
      </td>
    </tr>
    <% } %>
    </tfoot>
</table>